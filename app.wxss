page{
  width: 100%;
  height: 100%;
  background-color: rgb(238, 238, 254);
  /* background-color: #fff; */

}

.wrapper{
  display: flex;
  padding: 30rpx;
  flex-wrap: wrap;
  justify-content: center;
  align-items: center;
}


.wrapperItem{
  flex: 1 0 50%;
  height: 300rpx;
  margin: 20rpx;
  background-color: #fff;
  text-align: center;
  line-height: 300rpx;
}

.border-bottom-ddd{
  border-bottom: 1px solid #ddd;
}


.upload{
  position: fixed;
  width: 100% !important;
  left: 0;
  right: 0;
  bottom: 35rpx;
  border-radius: 15rpx;
}

.image-item{
  border: 1px solid black;
  width: 150rpx;
  height: 150rpx;
  margin: 3rpx;
}