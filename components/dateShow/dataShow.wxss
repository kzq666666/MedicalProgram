/* components/dateShow/dataShow.wxss */

.flex{
  display: flex;
  align-items: center;
  height: 60rpx;
  margin: 50rpx 0;
  
}
.leftIcon{
  margin: 20rpx;
}
.text{
  line-height: 16rpx;
  font-size: 32rpx;
}
.bottomArrow{
  transform: rotate(90deg);
}
.rightArrow{
  transform: rotate(0deg);
}
.image-item{
  border: 1px solid black;
  width: 150rpx;
  height: 150rpx;
  margin: 3rpx;
}

.imageShow{
  padding-left: 10rpx ;
}
.wrapperItem{
  border-bottom: 1px solid #ddd;
}

.noData{
  position: absolute;
  left: 50%;
  top: 40%;
  font-style: oblique;
  color: #909399;
  transform: translateX(-50%) translateY(-50%);   
}